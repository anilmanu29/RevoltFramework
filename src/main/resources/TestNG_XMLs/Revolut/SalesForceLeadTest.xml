<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SalesForce Test Suite">

  <listeners>
   	<listener class-name = "extentreportlistener.ExtentReporterNG"/>
   	<listener class-name = "testngcustomreportlistener.TestNGCustomReportListener"/>
    <listener class-name = "util.TestRetryAnalyzerListener"/>
  </listeners>

	<test name="SalesForce Account Creation Test">
	    <classes>
	         <class name="testcases.salesforce.combo.SalesforceComboLeadTest"></class>
	         <class name="testcases.salesforce.broker.SalesforceBrokerLeadTest"></class>
	         <class name="testcases.salesforce.carrier.SalesforceCarrierLeadTest"></class>
	         <class name="testcases.salesforce.shipper.SalesforceShipperLeadTest"></class>
	    </classes>
	</test> 
</suite> 